<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="review">
	<insert id="insertReview" parameterType="map">
	<!-- insert into info_review values(inforeviewNum, infoupNum, infoReviewContent, infoReviewPoint, info -->
		INSERT INTO INFO_REVIEW VALUES(SEQ_INFO_REVIEW_NUM.NEXTVAL, #{infoupNum}, #{infoReviewContent}, #{infoReviewPoint}, default, default)
	</insert>

</mapper>
